.form-wrapper {
width: 230px;
padding: 15px;
/*margin: 150px auto 50px auto;*/
/*background: #444;*/
/*background: rgba(0,0,0,.2);*/
background: rgba(216, 229, 255, 0.2);
border-radius: 10px;
/*box-shadow: 0 1px 1px rgba(0,0,0,.4) inset, 0 1px 0 rgba(255,255,255,.2);*/
}
.form-wrapper input{
    border:0;
    width:160px;
    height:25px;
}
.form-wrapper input:focus {
    outline: 0;
    background: #fff;
    /*box-shadow: 0 0 2px rgba(0,0,0,.8) inset;*/
    box-shadow: 0 0 1px #1F6DF7 inset;
}

.form-wrapper input::-webkit-input-placeholder {
   color: #999;
   font-weight: normal;
   font-style: italic;
}

.form-wrapper input:-ms-input-placeholder {
    color: #999;
    font-weight: normal;
    font-style: italic;
}

.form-wrapper input:-moz-placeholder {
    color: #999;
    font-weight: normal;
    font-style: italic;
}

.form-wrapper button {
overflow: visible;
position: relative;
/*float: right;*/
left:-7px;
border: 0;
padding: 0;
cursor: pointer;
height: 25px;
width: 60px;
font: bold 13px/24px 'lucida sans', 'trebuchet MS', 'Tahoma';
color: #fff;
text-transform: uppercase;
background: #1F96F7;
border-radius: 0 3px 3px 0;
text-shadow: 0 -1px 0 rgba(0, 0 ,0, .3);
}
.form-wrapper button:hover{   
    /*background: #e54040;*/
    background: #86BBFF;
}


.form-wrapper button:active,
.form-wrapper button:focus{   
/*    background: #c42f2f;*/
    background: #86BBFF;
  outline: 0;   
}

