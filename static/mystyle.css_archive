 /*   border-radius: 10px;*/

}

.header_nav ul, .ct1 ul{
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    font-size: medium;

}

.header_nav li{
    float: right;
    display: inline;
    font-size:14px;
    overflow:hidden;
}


.header_nav a:link, .header_nav a:visited, .header_nav a:hover {
    display: block;
    width:100px;
    font-weight: bold;
    color: #FFFFFF;
/*   background-color:#1F96F7;*/
    text-align: center;
    padding: 2px;
    text-decoration: none;
    border-radius: 5px;
/*/    text-transform: uppercase;*/
}

.header_nav .active {
    background-color:#86BBFF;
    border-radius: 5px;
}

